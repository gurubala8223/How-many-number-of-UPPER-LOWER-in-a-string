   "source": [
    "a=input()\n",
    "upper=0\n",
    "lower=0\n",
    "g=0\n",
    "for i in a:\n",
    "    if 65<=ord(i)<=90:\n",
    "        upper=upper+1\n",
    "    elif 97<=ord(i)<=122:\n",
    "        lower+=1\n",
    "    else:\n",
    "        g+=1\n",
    "     \n",
    "    \n",
    "print(upper,lower,g)        \n",
    "        "
   ]
